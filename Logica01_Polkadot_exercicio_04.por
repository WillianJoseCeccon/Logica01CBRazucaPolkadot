programa {
  funcao inicio() {
      real num01, num02, num03, media

      escreva("Digite o primeiro número :")
      leia(num01)

      escreva("Digite o segundo número :")
      leia(num02)

      escreva("Digite o terceiro número :")
      leia(num03)

      media = (num01 + num02 + num03) / 3

      escreva("A media dos números é ", media)

  }
}
